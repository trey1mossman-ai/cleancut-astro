---
// Google Reviews Section - Carousel style with equal-sized cards

// AggregateRating schema for star ratings in search results
const aggregateRatingSchema = {
  "@context": "https://schema.org",
  "@type": "LocalBusiness",
  "name": "Clean Cut Painting & Handyman",
  "image": "https://www.cleancutservice.com/images/CleanCut Image-3.webp",
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "1004 E Eldorado St",
    "addressLocality": "Decatur",
    "addressRegion": "IL",
    "postalCode": "62521"
  },
  "aggregateRating": {
    "@type": "AggregateRating",
    "ratingValue": "4.7",
    "reviewCount": "300",
    "bestRating": "5",
    "worstRating": "1"
  }
};

const reviews = [
  {
    name: "Tina Weakley",
    rating: 5,
    text: "We can't say enough good things about Clean Cut! Tyler, the Chief Revenue Officer and Manager, was absolutely amazingâ€”always quick to answer our calls, emails, and texts, and made sure everything ran smoothly from start to finish. Gustavo and Valeria were the painters, and they did a fantastic job! They painted the entire house ceiling, including the garage, and you'd never even know they'd been here except for how beautiful everything looks now.",
    services: ["Water damage repair", "Interior painting"],
    response: "Thank you for sharing your fantastic experience with Clean Cut! We're thrilled to hear that Tyler, Gustavo, and Valeria exceeded your expectations and delivered such outstanding results on your home. It's wonderful to know that the quality of our work transformed your space and addressed the water damage effectively. We look forward to helping you with your interior wall colors and making sure your next project is just as successful. Your trust in us means a lot, and we'll be ready to assist whenever you need!"
  },
  {
    name: "Brenda Thorp",
    rating: 5,
    text: "Tyler and his crew at Clean Cut did a great job at painting the exterior of my house. From the power washing, wood repair, prep work, painting and clean up everyone was professional and dependable. The house looks better than I expected.",
    services: ["Exterior painting"],
    response: "Thank you for your wonderful review, Brenda! We're thrilled to hear that you were pleased with the work Tyler and the team did on your home. It's great to know that the entire process met your expectations, from preparation to clean-up. Your recommendation means a lot to us, and we look forward to helping you with future projects!"
  },
  {
    name: "Paul Willis",
    rating: 5,
    text: "I hired Clean Cut to paint the windows and sills on my 2 story house. I think there were approximately 40 windows total. The painter was very professional, courteous, and thorough. He took time and care with the taping of the windows and the prep work. His finished work was excellent! He was very thorough with the clean up afterwards! I definitely would recommend them and plan on doing different projects in the future with them. Communication before, during, and afterwards with Clean Cut was excellent as well!",
    services: ["Exterior painting"],
    response: "Thank you for your wonderful feedback, Paul! We're thrilled to hear that you were satisfied with the painting of your windows and sills. Our team prides itself on professionalism and thoroughness, so it's great to know that reflected in your experience. We appreciate your recommendation and look forward to working with you on future projects!"
  },
  {
    name: "Jodi Ferriell",
    rating: 5,
    text: "Clean Cut painted my kitchen and hallway, power-washed my house and repaired some siding. The painters were very professional and did a great job! The power-washing and siding repair turned out well. Tyler was very helpful and communicated with me until the project was complete. I highly recommend Clean Cut for all your painting and handyman needs!",
    services: ["Interior painting", "Power washing"],
    response: "Thank you for your wonderful feedback, Jodi! We're thrilled to hear that you were pleased with the painting, power-washing, and siding repairs. It's great to know that our team, especially Tyler, provided you with the professional service and communication you deserve. Your recommendation means a lot to us, and we look forward to helping you with any future projects!"
  },
  {
    name: "Dalton Geisler",
    rating: 5,
    text: "Very happy with our deck staining project. Tyler came out and gave me an honest and straightforward quote, which I really appreciated. Dave and Lavern did the actual staining work on my deck and did a fantastic job. I'm very pleased with how it turned out and will definitely be calling them again for future projects.",
    services: ["Deck staining"],
    response: "Thank you for your kind words, Dalton! We're thrilled to hear that you had a positive experience with our team and that you're pleased with the results of your deck staining. We take pride in providing honest quotes and quality workmanship. We look forward to working with you again on your future projects!"
  },
  {
    name: "Katy Doolin",
    rating: 5,
    text: "Very polite and did a great job! George and his crew worked to make sure the job was done on time. I had huge holes in my drywall and they repaired and painted them and you would never know. Thank you!",
    services: ["Drywall repair", "Interior painting"],
    response: "Thank you for your kind words, Katy! We're thrilled to hear that George and his crew exceeded your expectations and delivered quality work on time. It's wonderful to know that the drywall repairs blend perfectly with your space. We appreciate your feedback and hope to assist you again in the future!"
  }
];

// Keywords to highlight
const highlightKeywords = [
  "fantastic job", "excellent", "highly recommend", "professional",
  "thorough", "great job", "on time", "quality", "dependable",
  "honest", "amazing", "incredible", "beautiful", "wonderful"
];

function highlightText(text: string): string {
  let result = text;
  highlightKeywords.forEach(keyword => {
    const regex = new RegExp(`(${keyword})`, 'gi');
    result = result.replace(regex, '<mark class="bg-sky-100 text-sky-800 px-1 rounded font-medium">$1</mark>');
  });
  return result;
}

function getInitials(name: string): string {
  return name.split(' ').map(n => n[0]).join('').toUpperCase();
}

function truncateText(text: string, maxLength: number): { truncated: string; isTruncated: boolean } {
  if (text.length <= maxLength) return { truncated: text, isTruncated: false };
  return { truncated: text.substring(0, maxLength).trim() + '...', isTruncated: true };
}
