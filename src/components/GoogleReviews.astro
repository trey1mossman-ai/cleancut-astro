<script>
  // Carousel functionality
  const track = document.getElementById('reviews-carousel-track');
  const prevBtn = document.getElementById('reviews-carousel-prev');
  const nextBtn = document.getElementById('reviews-carousel-next');
  const dots = document.querySelectorAll('.reviews-carousel-dot');
  const cards = track?.querySelectorAll('.review-card');

  let currentIndex = 0;
  let cardsPerView = 3;

  function updateCardsPerView() {
    if (window.innerWidth < 768) {
      cardsPerView = 1;
    } else if (window.innerWidth < 1024) {
      cardsPerView = 2;
    } else {
      cardsPerView = 3;
    }
  }

  function getMaxIndex() {
    return Math.max(0, (cards?.length || 0) - cardsPerView);
  }

  function updateCarousel() {
    if (!track || !cards?.length) return;

    const cardWidth = cards[0].offsetWidth;
    const gap = 24; // 6 * 4 = 24px gap
    const offset = currentIndex * (cardWidth + gap);
    track.style.transform = `translateX(-${offset}px)`;

    // Update dots
    dots.forEach((dot, i) => {
      const visual = dot.querySelector('.review-dot-visual');
      if (!visual) return;
      if (i === currentIndex) {
        visual.classList.remove('bg-gray-300', 'w-3');
        visual.classList.add('bg-[#4285F4]', 'w-4');
      } else {
        visual.classList.add('bg-gray-300', 'w-3');
        visual.classList.remove('bg-[#4285F4]', 'w-4');
      }
    });

    // Update button states
    if (prevBtn) prevBtn.disabled = currentIndex === 0;
    if (nextBtn) nextBtn.disabled = currentIndex >= getMaxIndex();
  }

  function goToSlide(index: number) {
    currentIndex = Math.max(0, Math.min(index, getMaxIndex()));
    updateCarousel();
  }

  // Event listeners
  prevBtn?.addEventListener('click', () => goToSlide(currentIndex - 1));
  nextBtn?.addEventListener('click', () => goToSlide(currentIndex + 1));

  dots.forEach((dot, index) => {
    dot.addEventListener('click', () => goToSlide(index));
  });

  // Initialize
  updateCardsPerView();
  updateCarousel();

  // Handle resize
  window.addEventListener('resize', () => {
    updateCardsPerView();
    currentIndex = Math.min(currentIndex, getMaxIndex());
    updateCarousel();
  });

  // Read more functionality
  document.querySelectorAll('.read-more-btn').forEach(btn => {
    btn.addEventListener('click', () => {
      const container = btn.closest('.review-text-container');
      const truncatedText = container?.querySelector('.review-text-truncated');
      const fullText = container?.querySelector('.review-text-full');

      if (truncatedText && fullText) {
        truncatedText.classList.toggle('hidden');
        fullText.classList.toggle('hidden');
        btn.textContent = truncatedText.classList.contains('hidden') ? 'Show less' : 'Read more';
      }
    });
  });

  // View response functionality
  document.querySelectorAll('.view-response-btn').forEach(btn => {
    btn.addEventListener('click', () => {
      const card = btn.closest('.review-card');
      const responseContainer = card?.querySelector('.response-container');
      const chevron = btn.querySelector('.response-chevron');
      const buttonText = btn.querySelector('span');

      if (responseContainer) {
        responseContainer.classList.toggle('hidden');
        chevron?.classList.toggle('rotate-180');
        if (buttonText) {
          buttonText.textContent = responseContainer.classList.contains('hidden')
            ? 'View response from Clean Cut'
            : 'Hide response';
        }
      }
    });
  });
</script>

<style>
  .review-card:hover {
    transform: translateY(-2px);
  }

  .response-chevron {
    transition: transform 0.3s ease;
  }

  .response-container {
    animation: slideDown 0.3s ease;
  }

  @keyframes slideDown {
    from {
      opacity: 0;
      max-height: 0;
    }
